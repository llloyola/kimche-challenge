{
	"trailingComma": "none",
	"tabWidth": 2,
	"printWidth": 100,
	"semi": true,
	"singleQuote": false,
	"useTabs": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"endOfLine": "lf"
}